  },
  "main": "js/app.js",
  "dependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-decorators": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "es5-shim": "^4.4.1",
    "expose-loader": "^0.7.1",
    "history": "^1.17.0",
    "node-libs-browser": "^0.5.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-on-rails": "../../..",
    "react-redux": "^4.0.5",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.2",
    "webpack": "^1.12.8"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "2.1.1",
    "eslint-plugin-react": "^3.13.1",
    "jscs": "^2.8.0"
  },
  "browser": {
    "fs": false
  },
  "scripts": {
    "lint": "npm run eslint --silent && npm run jscs --silent",
    "eslint": "eslint --ext .js,.jsx .",
    "jscs": "jscs --verbose .",
    "jscs:fix": "jscs --verbose -x . --silent",
    "build:test":  "webpack --config webpack.client.js && webpack --config webpack.server.js",
    "test": "npm run build:test && rspec",
    "build:dev:client": "webpack -w --config webpack.client.js",
    "build:dev:server": "webpack -w --config webpack.server.js",
    "build:client": "webpack --config webpack.client.js",
    "build:server": "webpack --config webpack.server.js"
  },
